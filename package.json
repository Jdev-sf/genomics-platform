{
  "name": "genomics-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "create:admin": "tsx scripts/create-admin.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "analyze": "cross-env ANALYZE=true next build",
    "security:audit": "better-npm-audit audit --level moderate",
    "security:audit-ci": "audit-ci --config .auditrc.json",
    "cleanup:guests": "tsx scripts/cleanup-guest-users.ts",
    "cache:init": "tsx scripts/init-cache.ts",
    "cache:warmup": "tsx -e \"import('./lib/cache/setup').then(({ CacheUtils }) => CacheUtils.warmupCache())\"",
    "cache:clear": "tsx -e \"import('./lib/cache/setup').then(({ CacheUtils }) => CacheUtils.clearAllCaches())\"",
    "cache:stats": "tsx -e \"import('./lib/cache/setup').then(({ CacheUtils }) => console.log(JSON.stringify(CacheUtils.getCacheStats(), null, 2)))\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@upstash/ratelimit": "^2.0.3",
    "@upstash/redis": "^1.28.4",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.13.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.511.0",
    "next": "^15.3.2",
    "next-auth": "^5.0.0-beta.28",
    "node-cache": "^5.1.2",
    "prisma": "^6.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "recharts": "^2.15.3",
    "reflect-metadata": "^0.2.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.17.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.17.50",
    "@types/node-cache": "^4.1.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/winston": "^2.4.4",
    "audit-ci": "^6.6.1",
    "better-npm-audit": "^3.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.0.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
